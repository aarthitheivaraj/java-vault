1. using array finding maximum value

import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int arr[]={12,34,56,23};
		int max=arr[0];
		
		for(int i=0;i<arr.length;i++){
		    if(arr[i]>max){
		        max=arr[i];
		    }
		}
		        System.out.print(max);
		    
		    
		
}
}

import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        String str1="Aarthi";
        String str2 = new String("Aarthi");
        char ch [] = {'A','a','r','t','h','i'};
        String str3 = new String(ch);
        System.out.println(str1);
        System.out.println(str2);
        System.out.println(str3);
        System.out.println();
    }
}
